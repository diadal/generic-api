{
  "name": "rafrsr/generic-api",
  "description": "API Abstraction layer with mocks",
  "license": "MIT",
  "type": "library",
  "homepage": "http://rafrsr.github.io/generic-api/",
  "authors": [
    {
      "name": "Rafael SR",
      "email": "raf.rsr@gmail.com",
      "homepage": "https://github.com/rafrsr"
    }
  ],
  "support": {
    "wiki": "https://github.com/rafrsr/generic-api/wiki"
  },
  "keywords": [
    "api",
    "sdk",
    "guzzle"
  ],
  "autoload": {
    "files": [
      "autoload.php"
    ],
    "psr-4": {
      "Rafrsr\\GenericApi\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Rafrsr\\GenericApi\\Tests\\": "tests/",
      "Rafrsr\\SampleApi\\": "sample/"
    }
  },
  "require": {
    "php": ">=5.5",
    "guzzlehttp/guzzle": "^6.0",
    "symfony/validator": "^2.3|^3.0",
    "doctrine/annotations": "^1.2",
    "doctrine/cache": "^1.4",
    "symfony/property-access": "^2.3|^3.0",
    "jms/serializer": "~0.11|~1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "4.1.*",
    "toplib/coding-standard": "dev-master",
    "satooshi/php-coveralls": "~1.0"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.0.x-dev"
    }
  }
}