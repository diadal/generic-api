{
  "name": "toplib/generic-api",
  "description": "API Abstraction layer with mocks",
  "license": "MIT",
  "type": "library",
  "homepage": "http://toplib.github.io/generic-api/",
  "authors": [
    {
      "name": "Rafael SR",
      "email": "raf.rsr@gmail.com",
      "homepage": "https://github.com/rafrsr"
    }
  ],
  "autoload": {
    "files": [
      "autoload.php"
    ],
    "psr-4": {
      "Toplib\\GenericApi\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Toplib\\GenericApi\\Tests\\": "tests/",
      "Toplib\\SampleApi\\": "sample/"
    }
  },
  "require": {
    "php": ">=5.4",
    "guzzlehttp/guzzle": ">=4,<6",
    "symfony/validator": "^2.7",
    "doctrine/annotations": "^1.2",
    "doctrine/cache": "^1.5",
    "symfony/property-access": "^2.7",
    "jms/serializer": "~0.11|~1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "4.1.*",
    "satooshi/php-coveralls": "dev-master",
    "toplib/coding-standard": "dev-master"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  }
}